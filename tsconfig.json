{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "baseUrl": "./src",
    "paths": {
      "@net/*": ["./net/*"],
      "components/*": ["./components/*"],
      "modules/*": ["./modules/*"],
      "assets/*": ["./assets/*"],
      "@pages/*": ["./pages/*"],
    },
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
}